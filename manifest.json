{
  "manifest_version": 3,
  "name": "Bitmap Re-direct",
  "version": "1.1",
  "description": "Redirects .bitmap URLs to ordinals content based on an index of the 1st 907k bitmaps",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["error.html", "redirect.html", "error.js", "redirect.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
